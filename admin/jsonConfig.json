{
  "i18n": true,
  "type": "panel",
  "items": {
    "apiKey": {
      "type": "text",
      "label": "api Key",
      "newLine": true,
      "sm": 6
    },
    "apiSecret": {
      "type": "password",
      "label": "api Secret",
      "newLine": false,
      "sm": 6
    },
    "plantId": {
      "type": "text",
      "label": "plantId",
      "help": "help_plantId",
      "newLine": true,
      "validator": "data.plantId.match(/^[0-9]([0-9])*$/)",
      "validatorErrorText": "errValplantId",
      "validatorNoSaveOnError": true
    },
    "pollInterval": {
      "type": "number",
      "label": "pollInterval",
      "help": "help_pollInterval",
      "newLine": true,
      "min": 45,
      "max": 1800
    }
  }
}

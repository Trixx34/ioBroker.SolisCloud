{
  "name": "iobroker.soliscloud",
  "version": "0.0.2",
  "description": "ioBroker adapter for reading values from soliscloud",
  "author": {
    "name": "Trixx34",
    "email": "Trixx34@mail.com"
  },
  "homepage": "https://github.com/Trixx34/ioBroker.SolisCloud.git",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "Solis",
    "template",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Trixx34/ioBroker.SolisCloud.git"
  },
  "engines": {
    "node": ">= 14"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.6",
    "iobroker.template": "^0.7.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.9",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.9",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@iobroker/adapter-dev": "^1.1.0",
    "@iobroker/testing": "^4.1.0",
    "@tsconfig/node14": "^14.1.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.30",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.24.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^5.0.5",
    "sinon": "^16.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~4.6.4"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "build-adapter ts",
    "watch": "build-adapter ts --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/Trixx34/ioBroker.SolisCloud/issues"
  },
  "readmeFilename": "README.md"
}
